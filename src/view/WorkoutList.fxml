<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="workoutField" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.WorkoutListController">

    <TableView fx:id="workoutTable" layoutX="40.0" layoutY="71.0" prefHeight="240.0" prefWidth="320.0">
        <columns>
            <TableColumn fx:id="indexCol" prefWidth="50.0" text="Index" />
            <TableColumn fx:id="workoutNameCol" prefWidth="200.0" text="Workout" />
            <TableColumn fx:id="calorieCol" prefWidth="70.0" text="Calorie" />
        </columns>
    </TableView>
    <Button fx:id="mainButton" layoutX="58.0" layoutY="478.0" mnemonicParsing="false" onAction="#mainButtonHandler" text="To Main" />
</AnchorPane>
