<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.VideoSearchController">
   <TextField fx:id="keywordTextField" layoutX="58.0" layoutY="73.0" prefHeight="35.0" prefWidth="200.0"
              promptText="Search Videos for"/>
   <Button fx:id="searchButton" layoutX="291.0" layoutY="78.0" mnemonicParsing="false" onAction="#searchButtonHandler"
           text="Search"/>
   <Label layoutX="58.0" layoutY="145.0" text="Suggested Videos"/>
   <ListView fx:id="videoListView" layoutX="58.0" layoutY="180.0" onMouseClicked="#videoMouseClickHandler"
             prefHeight="200.0" prefWidth="285.0"/>
   <Button fx:id="mainButton" layoutX="58.0" layoutY="478.0" mnemonicParsing="false" onAction="#mainButtonHandler"
           text="To Main"/>
</AnchorPane>
