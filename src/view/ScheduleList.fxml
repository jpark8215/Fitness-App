    <TableView fx:id="scheduleTable" layoutX="42.0" layoutY="72.0" prefHeight="240.0" prefWidth="320.0">
        <columns>
            <TableColumn fx:id="indexCol" prefWidth="50.0" text="Index" />
            <TableColumn fx:id="workoutNameCol" prefWidth="135.0" text="Workout" />
            <TableColumn fx:id="timeCol" prefWidth="135.0" text="Time" />
        </columns>
    </TableView>
    <Button fx:id="mainButton" layoutX="58.0" layoutY="478.0" mnemonicParsing="false" onAction="#mainButtonHandler" text="To Main" />
