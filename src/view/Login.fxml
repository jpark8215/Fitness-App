<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">

    <Label layoutX="129.0" layoutY="99.0" text="Trick9 Fitness" textAlignment="CENTER">
        <font>
            <Font size="24.0" />
        </font>
    </Label>
    <TextField fx:id="usernameField" layoutX="126.0" layoutY="275.0" promptText="Username" />
    <TextField fx:id="passwordField" layoutX="126.0" layoutY="311.0" promptText="Password" />
    <Button fx:id="loginButton" layoutX="163.0" layoutY="384.0" mnemonicParsing="false" onAction="#loginHandler" prefHeight="25.0" prefWidth="75.0" text="Login" textAlignment="CENTER" />
    <Label fx:id="timeZone" alignment="CENTER" contentDisplay="CENTER" layoutX="140.0" layoutY="163.0" prefHeight="17.0" prefWidth="120.0" text="User Location" textAlignment="CENTER" />
</AnchorPane>
