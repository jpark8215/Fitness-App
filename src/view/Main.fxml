<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
     <Label layoutX="25.0" layoutY="15.0" text="Select an option:" />
      <Button fx:id="addWorkoutButton" layoutX="25.0" layoutY="60.0" onAction="#addWorkout" text="Add workout" />
      <Button fx:id="removeWorkoutButton" layoutX="25.0" layoutY="100.0" onAction="#removeWorkout" text="Remove workout" />
      <Button fx:id="workoutByIndexButton" layoutX="25.0" layoutY="140.0" onAction="#workoutByIndex" text="Get workout by index" />
      <Button fx:id="workoutScheduleButton" layoutX="25.0" layoutY="180.0" onAction="#workoutSchedule" text="Show workout schedule" />
     <Button fx:id="videoSearchButton" layoutX="25.0" layoutY="220.0" onAction="#videoSearch" text="Video Search" />
     <Button fx:id="exitButton" layoutX="25.0" layoutY="350.0" onAction="#exit" text="Exit" />


</AnchorPane>
